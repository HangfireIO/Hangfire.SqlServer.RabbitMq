            <group targetFramework="net472">
                <dependency id="Hangfire.Core" version="1.7.0" />
                <dependency id="Hangfire.SqlServer" version="1.7.0" />
                <dependency id="RabbitMQ.Client" version="5.1.1" />
            </group>
            <group targetFramework="netstandard2.0">
                <dependency id="Hangfire.Core" version="1.7.0" />
                <dependency id="Hangfire.SqlServer" version="1.7.0" />
                <dependency id="RabbitMQ.Client" version="5.1.1" />
            </group>
        </dependencies>
    </metadata>
